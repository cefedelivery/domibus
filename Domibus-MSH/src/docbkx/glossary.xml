<glossary xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xml:id="glossary"
          version="5.0"
          xml:lang="en"
          xmlns="http://docbook.org/ns/docbook"
          xsi:schemaLocation="http://docbook.org/ns/docbook http://www.docbook.org/xml/5.0/xsd/docbook.xsd">
    <title>Glossary</title>
    <glossentry>
        <glossterm>$domibus.local.context</glossterm>
        <glossdef>
            <para>The local, unproxied gateway URL (defaults to<uri>
                http://localhost:8080/domibus</uri>)
            </para>
        </glossdef>
    </glossentry>
    <glossentry>
        <glossterm>$domibus.config.location2</glossterm>
        <glossdef>
            <para>The absolute path of the domibus configuration folder.</para>
        </glossdef>
    </glossentry>
    <glossentry>
        <glossterm>$domibus.install</glossterm>
        <glossdef>
            <para>The absolute path of the domibus installation folder.</para>
        </glossdef>
    </glossentry>
    <glossentry>
        <glossterm>$RDBMS</glossterm>
        <glossdef>
            <para>The name of the used database server, i.e.
                <glossterm>MySQL</glossterm>
            </para>
        </glossdef>
    </glossentry>
    <glossentry>
        <glossterm>e-CODEX</glossterm>
        <glossdef>
            <para>e-Justice Communication via Online Data EXchange</para>
        </glossdef>
    </glossentry>
    <glossentry>
        <glossterm>e-SENS</glossterm>
        <glossdef>
            <para>Electronic Simple European Networked Services</para>
        </glossdef>
    </glossentry>
    <glossentry>
        <glossterm>BOA</glossterm>
        <glossdef>
            <para>Backoffice application</para>
        </glossdef>
    </glossentry>
</glossary>
